# .gitattributes (推奨)
* text=auto

*.java       text eol=lf
*.xml        text eol=lf
*.properties text eol=lf
*.md         text eol=lf
*.html       text eol=lf
*.css        text eol=lf
*.js         text eol=lf

*.sh         text eol=lf
*.bat        text eol=crlf
*.cmd        text eol=crlf
*.ps1        text eol=crlf

.project     text eol=lf
.classpath   text eol=lf
.settings/** text eol=lf

.mvn/wrapper/maven-wrapper.properties text eol=lf
mvnw         text eol=lf
mvnw.cmd     text eol=crlf

*.jar    binary
*.war    binary
*.ear    binary
*.class  binary

.gitattributes text eol=lf